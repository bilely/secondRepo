package business;

public class Viande extends Proteine {
	
	String Animal ;
	String certification ;
	
	
	public Viande() {
		super();
	}
	
	public Viande(String nom) {
		super();
	}
	
	public Viande(String nom, String animal) {
		super();
		Animal = animal;
	}
	
	public Viande(String nom, String animal, String certification) {
		super();
		Animal = animal;
		this.certification = certification;
	}
	
	public String getAnimal() {
		return Animal;
	}
	public void setAnimal(String animal) {
		Animal = animal;
	}
	public String getCertification() {
		return certification;
	}
	public void setCertification(String certification) {
		this.certification = certification;
	}
	@Override
	public String toString() {
		return super.toString() + "Viande [" + (Animal != null ? "Animal=" + Animal + ", " : "")
				+ (certification != null ? "certification=" + certification : "") + "]";
	}
	
	
	
	
	

}
